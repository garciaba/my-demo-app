           "meTypes" : [ "PROCESS_GROUP_INSTANCE", "APPLICATION" ],
           "tags": [
                    {
                        "context": "CONTEXTLESS",
                        "key": "Stage",
                        "value": "Staging"
                    },
                    {
                        "context": "CONTEXTLESS",
                        "key": "Application",
                        "value": "EasyTravel"
                    }
                    ]
       }
   },
